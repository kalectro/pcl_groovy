-
-      if (kresult != kIOReturnSuccess || !libusb_darwin_mp)
-	return darwin_to_libusb (kresult);
-    }
-
-    pthread_create (&libusb_darwin_at, NULL, event_thread_main, (void *)ctx);
-
-    while (!libusb_darwin_acfl)
-      usleep (10);
-  }
-
-  return 0;
