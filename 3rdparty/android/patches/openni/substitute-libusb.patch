-		if (!to_cancel)
-			break;
-
-		usbi_backend->clear_transfer_priv(to_cancel);
-		usbi_handle_transfer_completion(to_cancel, LIBUSB_TRANSFER_NO_DEVICE);
-	}
-
-}
