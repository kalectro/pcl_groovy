-	if (timercmp(&cur_tv, next_timeout, >=)) {
-		usbi_dbg("first timeout already expired");
-		timerclear(tv);
-	} else {
-		timersub(next_timeout, &cur_tv, tv);
-		usbi_dbg("next timeout in %d.%06ds", tv->tv_sec, tv->tv_usec);
-	}
-
-	return 1;
-#else
-	return 0;
-#endif
-}
