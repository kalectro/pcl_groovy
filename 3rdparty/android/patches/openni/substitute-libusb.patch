-	 * continue event handling */
-	pthread_mutex_lock(&ctx->pollfd_modify_lock);
-	r = ctx->pollfd_modify;
-	pthread_mutex_unlock(&ctx->pollfd_modify_lock);
-	if (r) {
-		usbi_dbg("someone else is modifying poll fds");
-		return 0;
-	}
-
-	return 1;
-}
