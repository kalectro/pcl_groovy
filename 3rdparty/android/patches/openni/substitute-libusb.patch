-/* bulk continuation URB flag available from Linux 2.6.32 */
-static int check_flag_bulk_continuation(void)
-{
-	struct utsname uts;
-	int sublevel;
-
-	if (uname(&uts) < 0)
-		return -1;
-	if (strlen(uts.release) < 4)
-		return 0;
-	if (strncmp(uts.release, "2.6.", 4) != 0)
-		return 0;
-
-	sublevel = atoi(uts.release + 4);
-	return sublevel >= 32;
-}
